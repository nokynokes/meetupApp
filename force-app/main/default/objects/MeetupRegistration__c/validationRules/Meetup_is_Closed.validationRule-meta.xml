<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Meetup_is_Closed</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
  ISNEW(), 
  ISPICKVAL(Meetup__r.Status__c, &apos;Closed&apos;)
)</errorConditionFormula>
    <errorMessage>You cannot register for this meetup as it is closed</errorMessage>
</ValidationRule>
